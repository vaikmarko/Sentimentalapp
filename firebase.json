      {
        "source": "/**",
        "destination": "https://sentimentalapp-test-319737737925.europe-west1.run.app/**",
        "type": 301
      }
    ]
  },
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}