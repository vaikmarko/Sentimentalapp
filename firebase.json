      "**/.*",
      "**/node_modules/**",
      "venv/**",
      "*.py",
      "requirements.txt"
    ],
    "redirects": [
      {
        "source": "/app/**",
        "destination": "https://sentimentalapp-test-319737737925.europe-west1.run.app/app/**",
